import tornado.web
import json

class TestHandler(tornado.web.RequestHandler):
    def get(self):
        query = qs_decode(self.request.query)
        self.set_status(200)

        self.write(query)

    def post(self):
        data = json.loads(self.request.body)
        
        for key, value in data.iteritems():
            self.write(key + " " + value + "\n")
        
